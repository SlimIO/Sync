{
    "name": "@slimio/sync",
    "version": "0.1.0",
    "description": "SlimIO Projects Synchroniser/Manager",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "test": "cross-env psp && ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc npm test",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/Sync.git"
    },
    "keywords": [
        "slimio",
        "sync",
        "manager",
        "update"
    ],
    "files": [
        "bin/index.js",
        "commands/*.js",
        "src/*.js"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/Sync/issues"
    },
    "homepage": "https://github.com/SlimIO/Sync#readme",
    "dependencies": {
        "@iarna/toml": "^2.2.3",
        "@slimio/async-cli-spinner": "^0.5.1",
        "@slimio/lazy": "^1.2.0",
        "@slimio/lock": "^0.4.0",
        "@slimio/pretty-json": "^1.3.0",
        "@slimio/psp": "^0.11.0",
        "cliui": "^6.0.0",
        "dotenv": "^8.2.0",
        "fast-levenshtein": "^2.0.6",
        "fast-outdated": "^3.0.1",
        "fetch-github-repositories": "^2.0.0",
        "httpie": "^1.1.2",
        "isomorphic-git": "^0.78.2",
        "kleur": "^3.0.3",
        "lodash.pick": "^4.4.0",
        "make-promises-safe": "^5.1.0",
        "ms": "^2.1.2",
        "open": "^7.0.0",
        "qoa": "^0.2.0",
        "sade": "^1.7.3",
        "semver": "^7.1.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^4.0.0",
        "ava": "^3.1.0",
        "cross-env": "^7.0.0",
        "eslint": "^6.8.0",
        "husky": "^4.2.1",
        "jsdoc": "^3.6.3",
        "nyc": "^15.0.0",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=12"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint bin/index.js commands/**.js src/**.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    },
    "bin": {
        "psync": "./bin/index.js"
    }
}
