{
    "name": "@slimio/sync",
    "version": "0.1.0",
    "description": "SlimIO Projects Synchroniser/Manager",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "test": "cross-env psp && ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc npm test",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/Sync.git"
    },
    "keywords": [
        "slimio",
        "sync",
        "manager",
        "update"
    ],
    "files": [
        "bin/index.js",
        "commands/*.js",
        "src/*.js"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/Sync/issues"
    },
    "homepage": "https://github.com/SlimIO/Sync#readme",
    "dependencies": {
        "@slimio/async-cli-spinner": "^0.3.1",
        "@slimio/lazy": "^1.2.0",
        "@slimio/lock": "^0.2.0",
        "@slimio/psp": "^0.7.0",
        "dotenv": "^8.0.0",
        "fast-outdated": "^3.0.0",
        "fetch-github-repositories": "^1.0.1",
        "httpie": "^1.1.2",
        "isomorphic-git": "^0.60.1",
        "kleur": "^3.0.3",
        "make-promises-safe": "^5.0.0",
        "premove": "^1.0.0",
        "qoa": "^0.2.0",
        "sade": "^1.6.1",
        "semver": "^6.3.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.1.0",
        "@commitlint/config-conventional": "^8.1.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^3.0.3",
        "ava": "^2.2.0",
        "cross-env": "^5.2.0",
        "eslint": "^6.1.0",
        "husky": "^3.0.3",
        "jsdoc": "^3.6.3",
        "nyc": "^14.1.1",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint bin/index.js commands/**.js src/**.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    },
    "bin": {
        "psync": "./bin/index.js"
    }
}
