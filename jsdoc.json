{
    "opts": {
        "encoding": "utf8",
        "destination": "./jsdoc/",
        "recurse": true,
        "template": "node_modules/@escommunity/minami"
    },
    "source": {
        "include": [
            "./bin/index.js",
            "./commands/index.js",
            "./commands/install.js",
            "./commands/outdatedAll.js",
            "./commands/pspAll.js",
            "./commands/git.js",
            "./src/utils.js",
            "./src/cli-table.js"
        ],
        "exclude": [
            "node_modules",
            "client"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    }
}
